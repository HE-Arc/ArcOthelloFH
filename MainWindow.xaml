<Window x:Class="Orthello.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Orthello"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel x:Name="MainDockPanel" Margin="10,10,9.6,10">
            <Canvas Width="100" Background="Black" DockPanel.Dock="Left"/>
            <Canvas Width="100" Background="Black" DockPanel.Dock="Right"/>
        </DockPanel>
    </Grid>
</Window>
